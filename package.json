{
  "name": "@oxlint/migrate",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "type": "module",
  "bin": {
    "oxlint-migrate": "./dist/bin/oxlint-migrate.mjs"
  },
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "prepare": "husky",
    "generate": "node --import @oxc-node/core/register ./scripts/generate.ts",
    "format": "npx prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "build": "vite build",
    "manual-test": "pnpm build; chmod +x dist/bin/oxlint-migrate.mjs; npx . integration_test/projects/autoprefixer.eslint.config.mjs"
  },
  "files": [
    "dist/*",
    "README.md"
  ],
  "keywords": [
    "oxc",
    "oxlint",
    "eslint",
    "rules"
  ],
  "author": "Sysix <sysix@sysix-coding.de>",
  "license": "MIT",
  "devDependencies": {
    "@oxc-node/core": "^0.0.19",
    "@types/node": "^22.13.1",
    "eslint": "^9.20.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "oxlint": "^0.15.10",
    "prettier": "^3.5.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.5"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "globals": "^15.14.0"
  }
}
